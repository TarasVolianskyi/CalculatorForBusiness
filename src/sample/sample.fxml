<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" vgap="10"
          xmlns="http://javafx.com/javafx/8.0.121" fx:controller="sample.Controller">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="170.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="170.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label prefHeight="17.0" prefWidth="105.0" text="Висота жалюзі, см" textAlignment="CENTER"
                       GridPane.columnIndex="1"/>
                <Label text="Ширина жалюхі, см" GridPane.columnIndex="2"/>
                <TextField fx:id="widthTextField" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <TextField fx:id="heightTextField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="Кількість сонячних панелей" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <TextField fx:id="numOfPanel" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
                <Button fx:id="btnStart" mnemonicParsing="false" onAction="#getStart" prefHeight="25.0"
                        prefWidth="340.0" text="Порахувати загальну суму" GridPane.columnIndex="1"
                        GridPane.columnSpan="2" GridPane.rowIndex="4"/>
                <Label fx:id="lbOrder" text="" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <Label fx:id="lbPrice" text="" GridPane.columnIndex="2" GridPane.rowIndex="5"/>
                <Label fx:id="lbCountOfJalousies" text="" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
                <Label fx:id="lbPriceJalousies" text="" GridPane.columnIndex="2" GridPane.rowIndex="6"/>
                <Label fx:id="lbCountOfPanel" text="" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
                <Label fx:id="lbPriceOfPanel" text="" GridPane.columnIndex="2" GridPane.rowIndex="7"/>
                <Label fx:id="lbResultText" text="" GridPane.columnIndex="1" GridPane.rowIndex="8"/>
                <Label fx:id="lbResult" text="" GridPane.columnIndex="2" GridPane.rowIndex="8"/>
            </children>
        </GridPane>
    </children>
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
</GridPane>
